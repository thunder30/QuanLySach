
@{
    ViewBag.Title = "ListBook";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>List Books</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />

</head>
<body>
    <div>
        <h2>ListBook</h2>
        <ul>
            @{ int i = 1;}
            @foreach (var item in ViewBag.Books)
            {
                <li>@i  - <b>@item</b></li>
                i++;
            }
        </ul>
    </div>
</body>
</html>

